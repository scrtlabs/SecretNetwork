{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Constants",
  "type": "object",
  "required": [
    "decimals",
    "name",
    "symbol"
  ],
  "properties": {
    "decimals": {
      "type": "integer",
      "format": "uint8",
      "minimum": 0.0
    },
    "name": {
      "type": "string"
    },
    "symbol": {
      "type": "string"
    }
  }
}
