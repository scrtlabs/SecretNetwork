  "version": "0.2.0",
  "configurations": [
    {
      "name": "go-test",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${file}",
      "cwd": "${workspaceFolder}/x/compute/internal/keeper",
      "env": {
        "SGX_MODE": "SW",
        "RUST_BACKTRACE": "1"
      },
      "args": [
        "test",
        "-tags",
        "sgx",
        "-timeout",
        "1200s",
        "-p",
        "1",
        "-v",
        "./x/compute/internal/..."
      ]
    },
    {
      "name": "secretcli",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/cmd/secretd",
      "cwd": "${workspaceFolder}",
      "args": [
        "q",
        "compute",
        "tx",
        "9FAF715EA6671A17521FBC4D52AD391B72B6AADD102481B071DD80F34C50E79F",
        "--node",
        "http://localhost:26657",
        "--chain-id",
        "secretdev-1"
      ]
    }
  ]
}
